	<OriginalSource><![CDATA[// Constants for Datatype WORD
VAR_GLOBAL CONSTANT
	WORD_BYTE_ZZ : WORD := 16#0000;    // ZERO Constant
	SHFT_BYTE_00 : SINT := 0;    // shift 0 bits
	WORD_BYTE_00 : WORD := 16#00FF;    // LSB_Byte Constant
	SHFT_BYTE_01 : SINT := 8;    // shift 8 bits
	WORD_BYTE_01 : WORD := 16#FF00;    // MSB_Byte Constant
END_VAR

// Constants for Datatype DWORD
VAR_GLOBAL CONSTANT
	DWORD_BYTE_ZZ : DWORD := 16#00000000;    // ZERO Constant
	// shift 0 bits
	DWORD_BYTE_00 : DWORD := 16#000000FF;    // LSB_Byte Constant
	// shift 8 bits
	DWORD_BYTE_01 : DWORD := 16#0000FF00;    // MSB_Byte Constant
	SHFT_BYTE_02 : SINT := 16;    // shift 16 bits
	DWORD_BYTE_02 : DWORD := 16#00FF0000;    // MSB_Byte Constant
	SHFT_BYTE_03 : SINT := 24;    // shift 24 bits
	DWORD_BYTE_03 : DWORD := 16#FF000000;    // MSB_Byte Constant
END_VAR

]]></OriginalSource>
